{
  "name": "",
  "version": "",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell 'npm run js:dev' 'npm run css:dev'",
    "js:build": "browserify src/js/index.js --debug -p [minifyify --map index.js.map --output index.js.map] > app/index.js",
    "js:dev": "watchify src/js/index.js -v -d -o app/index.js",
    "css:build": "node-sass src/scss/style.scss | postcss -u autoprefixer > app/style.css",
    "css:dev": "onchange 'src/scss/**/*.scss' -- npm run css:build"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "exorcist": "^0.4.0",
    "minifyify": "^7.3.3",
    "node-sass": "^3.7.0",
    "onchange": "^2.4.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "firebase": "^3.0.4",
    "lodash.debounce": "^4.0.6",
    "prismjs": "^1.5.1",
    "remarkable": "^1.6.2",
    "rethinkdb": "^2.3.2",
    "shortid": "^2.2.6",
    "stockpile.js": "^1.2.0"
  }
}
